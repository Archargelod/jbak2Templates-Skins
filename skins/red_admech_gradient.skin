//----------
//Фон клавиш
//----------
//Стартовый цвет градиента для фона клавиши
KeyBackStartColor=0xffbf1b0f

//Конечный цвет градиента.
KeyBackEndColor=0xffdb6357

//Тип градиента. 0 - линейный, 1 - радиальный
KeyBackPressedGradientType=1

//Стартовый цвет градиента для фона клавиши в нажатом состоянии.
KeyBackPressedStartColor=0xff000000

//Конечный цвет градиента для фона клавиши в нажатом состоянии.
KeyBackPressedEndColor=0xff000000

//Тип градиента клавиш в нажатом состоянии. 0 - линейный, 1 - радиальный
KeyBackGradientType=1


//----------
//Текст на клавишах
//----------
//Цвет текста (надписей и значков на клавишах)
KeyTextColor=0xffffe6e6

//Жирный шрифт (1) или нет (0). Если не задано - не жирный
KeyTextBold=1

//Цвет дополнительного символа
KeySymbolColor=0xff4ab1e8

//Цвет текста в нажатом состоянии
KeyTextPressedColor=0xff00cc00

//Цвет символа в нажатом состоянии
KeySymbolPressedColor=0xff00cc00


//----------
//Обводка клавиш
//----------
//Стартовый цвет градиента для обводки клавиш.
KeyStrokeStartColor=0xffa89797

//Конечный цвет градиента для обводки клавиш.
KeyStrokeEndColor=0xff2e2221

//Стартовый цвет градиента для обводки клавиш в нажатом состоянии.
KeyPressedStrokeStartColor=0xffa89797

//Конечный цвет градиента для обводки клавиш в нажатом состоянии.
KeyPressedStrokeEndColor=0xff2e2221


//----------
//Фон клавиатуры
//----------
//Стартовый цвет градиента для фона клавиатуры
KeyboardBackgroundStartColor=0xff000000

//Конечный цвет градиента для фона клавиатуры.
KeyboardBackgroundEndColor=0xff000000

//Тип градиента. 0 - линейный, 1 - радиальный
KeyboardBackgroundGradientType=0


//----------
//Значения специальных клавиш (Shift, Backspace и др.).
//----------
//Цвет текста на кнопке
SpecKeyTextColor=0xfff2b200

//Цвет дополнительного символа
SpecKeySymbolColor=0xffffffff

//Цвет текста в нажатом состоянии
